ln -s ~/dotfiles/.config/kitty .config/kitty
ln -s ~/dotfiles/.config/powerline .config/powerline
ln -s ~/dotfiles/.config/starship.toml .config/starship.toml
ln -s ~/dotfiles/.config/fish/config.fish .config/config.fish
